  <title>Profile</title>
</head>
<body>
    {{> navbar}}
    <a class="my_profile" data-profile-id="{{my_status._id}}"></a>
    <div class="container-fluid row mt-4 justify-content-center">
      <div class="col-md-3 ms-3 text-center chatNamesHead" style="border: 2px solid white; color:white;">
        <h3>Chats</h3>
        <div class="chatNames" style="border: 2px solid white; color:white;">
          <ul style="list-style-type: none; padding: 0;">
            {{#each other_users}}
            <li class="userList" style="border-bottom: 1px solid #ccc; padding: 10px 0;" data-post-id="{{this._id}}">
              <div style="position: relative; display: inline-block;">
                <img src="/static/img/{{this.dp_image}}" style="border-radius: 50%; width: 30px; height: 30px; border:1px solid white" alt="{{this.name}}">
                {{#ifEquals this.is_online '1'}}
                  <span class="online-status" id="{{this._id}}-status"></span>
                  {{else}}
                  <span class="offline-status" id="{{this._id}}-status"></span>
                {{/ifEquals}}
              </div>
              <button id="chatsButton" style="border: none"><b>{{this.name}}</b></button>
            </li>
            {{/each}}
          </ul>
        </div>
      </div>

      <div class="col-md-8 ms-3 text-center initial" style="color:white; display: flex; align-items: center; justify-content: center;">
        <div style="text-align: center;">
          <b><i style="font-family: cursive;">Get Together</i></b>
          <p style="font-family:monospace">Chat with your Friends and Family...</p>
        </div>
      </div>


      <div class="col-md-8 ms-3 chatBody" style="border: 2px solid white; color:white; display:none">
